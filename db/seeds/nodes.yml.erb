    <%= OperatingSystem.find_by(name: "Windows Server SAC").id %>
  nics_attributes:
  - network_id: <%= Network.find_by(vlan: 101).id %>
    number: 1
    name: ""
    interface_type: wired
    locked: true
    ipv4_config: manual
    ipv4_address: "192.168.1.1"
